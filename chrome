    <!-- 검색 -->
<script>
    function performSearch() {
        var query = document.querySelector('.txt_search').value;
        var search1 = document.getElementById('search1').checked;
        var search2 = document.getElementById('search2').checked;
        var search3 = document.getElementById('search3').checked;

        if (query.trim() == '') {
            alert("검색어를 입력해주세요.");
            return;
        }

        if (search1) {
            window.location.href = '/game_info/item/list?SearchText=' + query;
            //window.open('/game_info/item/list?SearchText=' + query, '_blank')
        } else if (search2) {
            //window.location.href = 'https://webshop.elancia.nexon.com/Product/List/1?pnm=' + query;
            window.open('https://webshop.elancia.nexon.com/Product/List/1?pnm=' + query, '_blank')
        } else if (search3) {
            window.location.href = '/community/kin/list?SearchText=' + query;
            //window.open('/community/kin/list?SearchText=' + query, '_blank')
        }
    }
</script>
<div id="search_area">
    <div class="tab">
        <input type="radio" name="flag" id="search1" checked=""><label for="search1" class="search1">아이템</label>
        <input type="radio" name="flag" id="search2"><label for="search2" class="search2">캐시아이템</label>
        <input type="radio" name="flag" id="search3"><label for="search3" class="search3">지식인</label>
    </div>
    <fieldset>
        <input type="text" name="search" class="txt_search"><button type="button" onclick="performSearch()" class="btn_search">검색</button>
    </fieldset>
</div>


